{
    "name": "@cratis/applications.react.mvvm",
    "version": "1.0.0",
    "description": "",
    "author": "Cratis",
    "license": "MIT",
    "repository": "github:cratis/applicationmodel",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist",
        "**/*.ts",
        "**/*.tsx"
    ],
    "type": "module",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "types": "./dist/esm/index.d.ts"
        },
        "./browser": {
            "import": "./dist/esm/browser/index.js",
            "require": "./dist/cjs/browser/index.js",
            "types": "./dist/esm/browser/index.d.ts"
        },
        "./messaging": {
            "import": "./dist/esm/messaging/index.js",
            "require": "./dist/cjs/messaging/index.js",
            "types": "./dist/esm/messaging/index.d.ts"
        }
    },
    "scripts": {
        "prepare": "yarn g:build",
        "clean": "yarn g:clean",
        "build": "yarn g:build",
        "lint": "yarn g:lint",
        "lint:ci": "yarn g:lint:ci",
        "test": "yarn g:test",
        "ci": "yarn g:ci",
        "up": "yarn g:up"
    },
    "dependencies": {
        "@cratis/applications": "1.0.0",
        "@cratis/fundamentals": "5.0.8",
        "mobx": "6.12.3",
        "mobx-react": "9.1.1",
        "react": "18.3.1",
        "reflect-metadata": "0.2.2",
        "tsyringe": "4.8.0"
    },
    "devDependencies": {
        "@types/react": "18.3.2",
        "@types/react-dom": "18.3.0"
    }
}
